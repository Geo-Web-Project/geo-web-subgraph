{
  "name": "geo-web-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ Geo-Web-Project/geo-web-xdai-subgraph",
    "create-local": "graph create --node http://localhost:8020/ Geo-Web-Project/geo-web-xdai-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ Geo-Web-Project/geo-web-xdai-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 Geo-Web-Project/geo-web-xdai-subgraph",
    "prepare:kovan": "mustache config/kovan.json subgraph.template.yaml > subgraph.yaml",
    "prepare:sokul": "mustache config/sokul.json subgraph.template.yaml > subgraph.yaml",
    "prepare:xdai": "mustache config/xdai.json subgraph.template.yaml > subgraph.yaml",
    "prepare:local": "mustache config/local.json subgraph.template.yaml > subgraph.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.19.0",
    "@graphprotocol/graph-ts": "0.19.0",
    "as-geo-web-coordinate": "^0.6.0",
    "mustache": "^3.1.0"
  }
}
